{
  "name": "Web timer extension",
  "version": "1.0",
  "description": "Shows you screen time stats.",
  "permissions": ["declarativeContent", "storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/timer16.png",
      "32": "images/timer32.png",
      "128": "images/timer128.png"
    }
  },
  "icons": {
    "16": "images/timer16.png",
    "32": "images/timer32.png",
    "128": "images/timer128.png"
  },
  "manifest_version": 2
}